{
	"manifest_version": 2,
	"name": "Timecode Saver",
	"description": "Save video timecode",
	"version": "1.0.0",
	"browser_action": {
		"default_icon": "timecode.png",
		"default_popup": "popup.html"
	},
	"content_scripts": [{
		"matches": ["*://vk.com/video*"],
		"js": ["vk_video.js"]
	}],
	"permissions": [
		"activeTab"
	]
}
